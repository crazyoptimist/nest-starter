{
  "name": "nest-starter",
  "version": "2.1.0",
  "description": "CarmaTech API based on NestJS framework",
  "author": "tonytan4ever<tonytan198211@gmail.com> and Abdullah Abid",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "nest build --webpack --webpackPath webpack-hmr.config.js --watch",
    "start:dev": "nodemon",
    "start:debug": "nodemon --config nodemon-debug.json",
    "prestart:prod": "rimraf dist && tsc",
    "start:prod": "node -r ./tsconfig-paths-bootstrap.js dist/main.js",
    "start:hmr": "node dist/server",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "IS_TS_NODE=true ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli -d typeorm.config.ts",
    "db:migrate": "npm run typeorm migration:generate src/migrations/$npm_config_name",
    "db:upgrade": "yarn typeorm migration:run",
    "db:downgrade": "yarn typeorm migration:revert",
    "db:drop": "npm run typeorm schema:drop",
    "prepare": "husky install",
    "deploy": "npm install && npm run build && npm run start:dev"
  },
  "dependencies": {
    "@aws-lambda-powertools/logger": "^1.12.1",
    "@aws-sdk/client-dynamodb": "^3.321.1",
    "@aws-sdk/client-lambda": "^3.398.0",
    "@aws-sdk/client-sns": "^3.430.0",
    "@aws-sdk/lib-dynamodb": "^3.321.1",
    "@aws-sdk/smithy-client": "^3.374.0",
    "@nest-dynamodb/typedorm": "^1.0.1",
    "@nestjs/axios": "^3.0.0",
    "@nestjs/common": "^9.4.3",
    "@nestjs/config": "^2.3.4",
    "@nestjs/core": "^9.4.3",
    "@nestjs/jwt": "^10.1.0",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "^9.4.3",
    "@nestjs/swagger": "^6.3.0",
    "@nestjs/typeorm": "^9.0.1",
    "@typedorm/common": "^1.15.4",
    "@typedorm/core": "^1.15.4",
    "@vendia/serverless-express": "^4.10.1",
    "aws-sdk": "^2.1367.0",
    "axios": "^1.5.1",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dotenv": "^16.3.1",
    "lint-staged": "^13.3.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "source-map-support": "^0.5.21",
    "swagger-ui-express": "^4.6.3",
    "tsconfig-paths": "^4.1.0",
    "typeorm": "^0.3.17"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.5.0",
    "@nestjs/schematics": "^9.2.0",
    "@nestjs/testing": "^9.4.3",
    "@types/aws-lambda": "^8.10.119",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.6",
    "@types/node": "^18.17.12",
    "@types/passport-jwt": "^3.0.9",
    "@types/source-map-support": "^0.5.7",
    "@types/supertest": "^2.0.14",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.3",
    "jest": "^29.6.4",
    "prettier": "^2.8.8",
    "run-script-webpack-plugin": "^0.2.0",
    "supertest": "6.3.3",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "webpack": "^5.88.2",
    "webpack-node-externals": "^3.0.0"
  },
  "_moduleAliases": {
    "@app": "./dist",
    "@modules": "./dist/modules"
  }
}
